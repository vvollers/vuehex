{
	"name": "vuehex",
	"version": "0.5.2",
	"private": false,
	"description": "A fast and flexible Vue 3 component for displaying binary data as a hexadecimal view",
	"author": "Vincent Vollers",
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./styles": {
			"import": "./dist/styles.js",
			"types": "./dist/styles.d.ts"
		},
		"./style.css": "./dist/style.css",
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE.txt"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
		"build:demo": "vite build --mode demo",
		"preview": "vite preview",
		"storybook": "storybook dev -p 6006 --disable-telemetry",
		"storybook:build": "storybook build --output-dir dist-storybook",
		"lint": "biome check",
		"type-check": "vue-tsc --noEmit",
		"format": "biome check --write",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {},
	"peerDependencies": {
		"vue": "^3.5.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@storybook/addon-docs": "^10.1.11",
		"@storybook/addon-links": "^10.1.11",
		"@storybook/vue3-vite": "^10.1.11",
		"@types/node": "^25.0.3",
		"@vitejs/plugin-vue": "^6.0.3",
		"@vue/tsconfig": "^0.8.1",
		"storybook": "^10.1.11",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@7.2.5",
		"vue": "^3.5.26",
		"vue-tsc": "^3.1.8"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@7.2.5"
	},
	"keywords": [
		"vue",
		"vue3",
		"hex",
		"hex-view",
		"hex-editor",
		"binary",
		"component",
		"virtualized"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/vvollers/vuehex.git"
	},
	"bugs": {
		"url": "https://github.com/vvollers/vuehex/issues"
	},
	"homepage": "https://vvollers.github.io/vuehex/"
}
